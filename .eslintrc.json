{
  "plugins": ["import"],
  "extends": ["next/core-web-vitals", "prettier"],
  "rules": {
    "semi": ["error", "always"],
    "indent": [
      "error",
      2,
      {
        "SwitchCase": 1
      }
    ],
    "quotes": ["error", "double"],
    "brace-style": ["error", "stroustrup"],
    "no-console": [
      "warn",
      {
        "allow": ["warn", "error"]
      }
    ],
    "comma-dangle": ["warn", "never"],
    "no-unused-vars": ["warn"],
    // "import/newline-after-import": [
    //   "error",
    //   {
    //     "count": 2
    //   }
    // ]
  }
}
